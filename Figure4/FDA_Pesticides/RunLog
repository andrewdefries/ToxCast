> DoTheQuery<-function(a){
+ drugTargets <- activeTargets(pubChemDatabase, "124511")
+ targetSequences <- read.GenBank(row.names(drugTargets), species.names = TRUE)
+ drugTargetSpecies<-cbind(attr(targetSequences, "species"), names(targetSequences))
+ ############
+ write.table(drugTargets, file=paste("PCID_", "124511", "_drugTargets", sep=""), col.names=F, row.names=F, quote=F)
+ write.table(unique(drugTargetSpecies[,1]), file=paste("PCID_", "124511", "_drugTargetSpecies", sep=""), col.names=F, row.names=F, quote=F)
