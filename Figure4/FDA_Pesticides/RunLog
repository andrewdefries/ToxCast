> sampleDatabasePath <- file.path(getwd(), "bioassayDatabaseWithSpecies.sqlite")
> 
> pubChemDatabase <- connectBioassayDB(sampleDatabasePath)
> 
> ############
> ##query<-DrugBank4Now[,1]
> #query<-"2244"
> #query<-"132970"
> ############
> ############
> DoTheQuery<-function(a){
+ drugTargets <- activeTargets(pubChemDatabase, "19519")
+ targetSequences <- read.GenBank(row.names(drugTargets), species.names = TRUE)
+ drugTargetSpecies<-cbind(attr(targetSequences, "species"), names(targetSequences))
+ ############
+ write.table(drugTargets, file=paste("PCID_", "19519", "_drugTargets", sep=""), col.names=F, row.names=F, quote=F)
+ write.table(unique(drugTargetSpecies[,1]), file=paste("PCID_", "19519", "_drugTargetSpecies", sep=""), col.names=F, row.names=F, quote=F)
+ write.table(targetSequences, file=paste("PCID_", "19519", "_drugTargetSequences", sep=""), col.names=F, row.names=F, quote=F)
+ ############
+ }
> ############
> a<-1#:length(query)
