> DoTheQuery<-function(a){
+ drugTargets <- activeTargets(pubChemDatabase, "57507194")
+ targetSequences <- read.GenBank(row.names(drugTargets), species.names = TRUE)
+ drugTargetSpecies<-cbind(attr(targetSequences, "species"), names(targetSequences))
+ ############
+ write.table(drugTargets, file=paste("PCID_", "57507194", "_drugTargets", sep=""), col.names=F, row.names=F, quote=F)
+ write.table(unique(drugTargetSpecies[,1]), file=paste("PCID_", "57507194", "_drugTargetSpecies", sep=""), col.names=F, row.names=F, quote=F)
+ write.table(targetSequences, file=paste("PCID_", "57507194", "_drugTargetSequences", sep=""), col.names=F, row.names=F, quote=F)
+ ############
+ }
> ############
